{
    "name": "@mevron/web-app",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "start": "next dev",
        "android": "expo start --android",
        "ios": "expo start --ios",
        "web": "expo start --web",
        "eject": "expo eject",
        "build": "next build",
        "expo": "expo start:web",
        "expo:build": "expo build:web",
        "optimize": "expo optimize",
        "gatsby:build": "yarn gatsby build",
        "test": "react-app-rewired test",
        "format": "prettier --write --config ../../.prettierrc.js --ignore-path ../../.prettierignore '**/*.{js,json,js,ts,tsx}'",
        "lint": "eslint --ext .js,.jsx,.ts,.tsx --ignore-path ../../.eslintignore .",
        "build:web": "expo build:web && expo-optimize"
    },
    "dependencies": {
        "@mevron/cross-core": "^0.0.1",
        "@mevron/cross-page": "^0.0.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-native-web": "0.17.1",
        "next": "9.3.5",
        "next-fonts": "1.5.1",
        "react-router-dom": "^6.0.2",
        "react-responsive": "^8.2.0",
        "embla-carousel": "^4.5.3",
        "exenv": "^1.2.0",
        "react-lifecycles-compat": "^3.0.0",
        "warning": "^4.0.3",
        "rambda": "6.5.3",
        "auto-bind": "4.0.0",
        "hoist-non-react-statics": "^3.3.2",
        "react-hook-form": "6.15.4",
        "react-app-polyfill": "2.0.0",
        "@hookstate/core": "3.0.6",
        "react-addons-shallow-compare": "15.6.3",
        "fuse.js": "6.4.6",
        "peerjs": "1.3.2",
        "react-redux": "7.2.4",
        "@react-google-maps/api": "2.2.0",
        "framer-motion": "^4.1.17",
        "react-datepicker": "4.3.0",
        "mdbreact": "5.2.0",
        "react-icons": "^4.2.0",
        "bootstrap": "5.1.0",
        "bootstrap-extended": "1.1.0",
        "react-select": "5.2.1",
        "@coreui/react": "4.0.2",
        "@coreui/icons": "2.1.0",
        "@coreui/coreui": "4.0.5",
        "@coreui/icons-react": "2.0.0"
    },
    "devDependencies": {
        "@testing-library/jest-dom": "5.11.2",
        "@testing-library/react": "10.4.7",
        "@testing-library/user-event": "12.0.17",
        "@types/react-dom": "16.9.8",
        "react-app-rewired": "2.1.6",
        "react-scripts": "4.0.3",
        "http-proxy-middleware": "^1.0.6",
        "react-app-rewire-alias": "^0.1.9",
        "customize-cra": "1.0.0"
    },
    "eslintConfig": {
        "extends": "../../.eslintrc.js",
        "rules": {
            "import/no-webpack-loader-syntax": [
                "error"
            ]
        },
        "overrides": [
            {
                "files": [
                    "config-overrides.js"
                ],
                "rules": {
                    "@typescript-eslint/no-var-requires": "off"
                }
            }
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all",
            "not ie < 11",
            "ie 11"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version",
            "ie 11"
        ]
    }
}
